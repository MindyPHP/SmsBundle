{
    "name": "mindy/sms-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "zelenin/smsru": "~4"
    },
    "require-dev": {
        "mindy/orm": "^4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/console": "^3.4",
        "symfony/framework-bundle": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\SmsBundle\\": ""
        },
        "exclude-from-classmap": [
            "./Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\SmsBundle\\Tests\\": "./Tests/"
        }
    }
}
